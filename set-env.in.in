#! /bin/sh

_s=@abs_top_builddir@/tmp/staging/`uname -m`-linux
PATH=$PATH:/usr/local/sbin:/usr/sbin:/sbin:$_s/usr/bin
CCACHE_DIR=@CCACHE_DIR@
BBPATH=@abs_top_builddir@:@ELITO_ROOTDIR@/de.sigma-chemnitz:@ELITO_ROOTDIR@/org.openembedded
ELITO_METRICS_ID=$$

unset MAKEFLAGS
unset MAKEOVERRIDES
unset POSIXLY_CORRECT
export ELITO_METRICS_ID
export CCACHE_DIR BBPATH

for f in @abs_top_builddir@/set-env.local $HOME/.elito-set-env; do
    test -r "$f" || continue
    . "$f"
done
